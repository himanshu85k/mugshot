{
  "appId": "com.moontrail.mugshot",
  "appName": "mugshot",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "build",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0,
      "backgroundColor": "#12121200",
      "androidScaleType": "CENTER_CROP",
      "androidSpinnerStyle": "large",
      "spinnerColor": "#121212",
      "androidSplashResourceName": "splash",
      "showSpinner": true
    }
  },
  "cordova": {}
}
